{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev:stylelint": "stylelint styles/*.css --fix",
    "dev:postcss": "postcss styles --dir app/styles --watch",
    "dev:remix": "remix watch",
    "dev": "run-p dev:*",
    "eslint": "eslint . --fix",
    "postinstall": "remix setup cloudflare-workers",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "start": "miniflare --https-key ./192.168.100.100-key.pem --https-cert ./192.168.100.100.pem --build-command \"npm run dev:worker\" --live-reload",
    "deploy": "npm run build && wrangler publish"
  },
  "browserslist": [
    "last 3 versions",
    "> 2%"
  ],
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@remix-run/cloudflare-workers": "^1.1.3",
    "@remix-run/node": "^1.3.2",
    "@remix-run/react": "^1.1.3",
    "@supabase/supabase-auth-helpers": "^1.3.0",
    "@supabase/supabase-js": "^1.32.2",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "fp-ts": "^2.11.10",
    "purecss-sass": "^2.1.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-router-dom": "^6.2.2",
    "remix": "^1.1.3",
    "remix-utils": "^2.10.0",
    "skyway-js": "^4.4.3",
    "ts-pattern": "^4.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.5",
    "esbuild": "0.13.14",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-nested": "^5.0.6",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.1.2",
    "vitest": "^0.9.3",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
